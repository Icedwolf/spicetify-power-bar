    "copyfiles": "^2.4.1",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.28.0",
    "spicetify-creator": "^1.0.14"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash-es": "^4.17.21",
